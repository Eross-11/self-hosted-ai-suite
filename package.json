{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "server": "node src/api/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "seed": "node scripts/seed.js",
    "seed:users": "node scripts/seedUsers.js",
    "dev:setup": "node scripts/devSetup.js",
    "devsetup": "npm run dev:setup",
    "audit": "npm audit --audit-level=moderate || true",
    "lighthouse": "npx lighthouse http://localhost:5173 --quiet --chrome-flags='--headless' --output html --output-path ./lighthouse-report.html",
    "analytics:posthog": "echo 'Integrate PostHog in your app for analytics'"
  },
  "dependencies": {
    "@devcycle/react-client-sdk": "^1.40.1",
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@supabase/supabase-js": "^2.39.7",
    "express": "^5.1.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^14.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.158.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "browserslist": "^4.25.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^5.14.0",
    "supabase": "^1.200.3",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
